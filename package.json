{
  "name": "@rbxts/fitumi",
  "version": "1.1.0",
  "description": "Fake It 'Till You Make It - A unit testing utility for faking everything from Roblox Instances to custom objects.",
  "main": "out/init.lua",
  "scripts": {
    "eslint": "npx eslint \"src/**/*.ts\" --max-warnings 0",
    "prepublishOnly": "rm -rf ./out; rbxtsc --type=package",
    "test": "rojo build -o tests/build.rbxlx ./tests/default.project.json && run-in-roblox ./tests/build.rbxlx -s ./tests/spec.server.lua",
    "wsl-open-unit-tests": "set -e; rojo build -o ./tests/build.rbxlx ./tests/default.project.json; ./scripts/open-in-windows ./tests/build.rbxlx",
    "wsl-run-unit-tests": "set -e; rojo build -o ./tests/build.rbxlx ./tests/default.project.json; ./scripts/run-unit-tests-in-windows ./tests/build.rbxlx ./tests/spec.server.lua"
  },
  "keywords": [],
  "author": "Bytebit",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bytebit-Org/fitumi.git"
  },
  "bugs": {
    "url": "https://github.com/Bytebit-Org/fitumi/issues"
  },
  "homepage": "https://github.com/Bytebit-Org/fitumi#readme",
  "publishConfig": {
    "access": "public"
  },
  "files": [
	"out/**/*.d.ts",
	"out/**/*.lua"
  ],
  "types": "out/index.d.ts",
  "devDependencies": {
    "@rbxts/compiler-types": "^2.0.4-types.1",
    "@rbxts/types": "^1.0.656",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-roblox-ts": "^0.0.35",
    "prettier": "^2.8.2",
    "typescript": "^4.9.5"
  }
}
